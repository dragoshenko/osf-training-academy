<custom-attribute attribute-id="body" xml:lang="x-default"><style type="text/css"> .services-container { background-color: #f5f5f5; padding: 30px 20px; font-family: Arial, sans-serif; width: 100%; } .services-row { display: flex; justify-content: space-between; max-width: 1200px; margin: 0 auto; } .service-column { flex: 1; display: flex; flex-direction: column; align-items: center; padding: 10px; } .service-title { font-size: 22px; margin-bottom: 20px; font-weight: bold; text-align: center; } .trusted-img { max-width: 114px; height: auto; } .payment-img { max-height: 60px; margin: 5px; } .mastercard-img { max-height: 70px; margin: 5px; } .payment-container { display: flex; flex-direction: column; align-items: center; } .payment-row { display: flex; justify-content: center; align-items: center; } /* Updated shipping container to ensure horizontal layout */ .shipping-container { display: flex; flex-direction: row; justify-content: center; align-items: center; gap: 15px; } /* Reduced the height of shipping images to ensure they fit side by side */ .shipping-img { height: 80px; width: auto; max-width: 120px; } @media (max-width: 768px) { .services-row { flex-direction: column; } .service-column { margin-bottom: 30px; } /* Make shipping images stack on mobile if needed */ .shipping-container { flex-direction: column; } } </style> <div class="services-container"> <div class="services-row"> <!-- Trusted Shops Column --> <div class="service-column"> <div class="service-title">Trusted Shops</div> <img alt="Trusted Shops" src="Trusted%20shops.png?$staticlink$" width="114" height="114" /> </div> <!-- Payment Methods Column --> <div class="service-column"> <div class="service-title">Payment Methods</div> <div class="payment-container"> <div class="payment-row"> <img alt="Visa" src="Visa.jpg?$staticlink$" width="154" height="98" /> <img alt="PayPal" src="paypal.png?$staticlink$" width="209" height="105" /> </div> <img alt="MasterCard" src="MasterCard.jpg?$staticlink$" width="161" height="102" /> </div> </div> <!-- Shipping Services Column --> <div class="service-column"> <div class="service-title">Shipping Services</div> <div class="shipping-container"> <img alt="Bike Delivery" src="201610872016-03-314963542BikeDelivery.jpg?$staticlink$" width="140" height="100" /> <img alt="Delivery Service" src="delivery-logo-design_1465-84.jpg?$staticlink$" width="140" height="100" /> </div> </div> </div> </div></custom-attribute>
